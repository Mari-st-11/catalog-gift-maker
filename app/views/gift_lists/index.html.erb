<div class="">

<!-- ギフトリスト一覧 -->

  <% if @gift_lists.present? %>
    <ul class="menu">
      <% @gift_lists.each do |giftlist| %>
        <li>
          <%= link_to giftlist.resource_name, '#' %>
        </li>
      <% end %>
    </ul>
  <% else %>
    <div class="">
      ギフトリストはまだありません<br>
      ギフトリストを作成しましょう！
    </div>
  <% end %>

</div>